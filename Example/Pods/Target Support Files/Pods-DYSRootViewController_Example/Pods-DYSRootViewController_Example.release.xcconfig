FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/DYSCategories.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/DYSHelpers.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/DYSRootViewController.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/MBProgressHUD.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/MJRefresh.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "DYSCategories" -framework "DYSHelpers" -framework "DYSRootViewController" -framework "MBProgressHUD" -framework "MJRefresh"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-DYSRootViewController_Example
PODS_ROOT = ${SRCROOT}/Pods